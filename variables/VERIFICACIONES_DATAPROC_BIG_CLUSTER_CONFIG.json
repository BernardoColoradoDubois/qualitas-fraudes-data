{
  "gce_cluster_config": {
    "internal_ip_only": true,
    "subnetwork_uri": "projects/shared-nonprod-eba6/regions/us-central1/subnetworks/qlts-svpc-non-prd-sn",
    "service_account": "dataproc-dev-operaciones@qlts-nonprod-data-tools.iam.gserviceaccount.com",
    "shielded_instance_config": {
      "enable_secure_boot": false,
      "enable_vtpm": false,
      "enable_integrity_monitoring": false
    }
  },
  "master_config": {
    "num_instances": 1,
    "machine_type_uri": "n2-highmem-8", 
    "disk_config": {
      "boot_disk_type": "pd-ssd", 
      "boot_disk_size_gb": 100  
    }
  },
  "worker_config": {
    "num_instances": 20,  
    "machine_type_uri": "n2-highmem-8",  
    "disk_config": {
      "boot_disk_type": "pd-ssd", 
      "boot_disk_size_gb": 100 
    }
  },
  "secondary_worker_config": {
    "num_instances": 0, 
    "is_preemptible": false
  },
  "software_config": {
    "image_version":"2.1.85-debian11",
    "properties": {
      "spark:spark.scheduler.mode": "FAIR",
      "spark:spark.executor.memory": "8g",  
      "spark:spark.driver.memory": "16g",   
      "spark:spark.executor.cores": "2",   
      "spark:spark.executor.memoryOverhead": "4g",  
      
      "spark:spark.dynamicAllocation.enabled": "true",
      "spark:spark.dynamicAllocation.minExecutors": "12",  
      "spark:spark.dynamicAllocation.maxExecutors": "44", 
      "spark:spark.dynamicAllocation.initialExecutors": "20", 
      
      

      "yarn:yarn.nodemanager.resource.memory-mb": "55840",  
      "yarn:yarn.scheduler.maximum-allocation-mb": "555840",
      "capacity-scheduler:yarn.scheduler.capacity.resource-calculator": "org.apache.hadoop.yarn.util.resource.DefaultResourceCalculator",
      
      "spark:spark.sql.execution.arrow.maxRecordsPerBatch": "30000",
      "spark:spark.sql.sources.parallelPartitionDiscovery.threshold": "48",
      "spark:spark.sql.adaptive.advisoryPartitionSizeInBytes": "134217728", 
      "spark:spark.sql.sources.partitionOverwriteMode": "dynamic",
      "spark:spark.hadoop.parquet.memory.pool.ratio": "0.3",
      
      "spark:spark.yarn.am.memory": "15g",  
      "spark:spark.task.maxFailures": "8",
      "spark:spark.stage.maxConsecutiveAttempts": "4",
      "spark:spark.locality.wait": "10s",
      "spark:spark.shuffle.service.enabled": "true",
      "dataproc:dataproc.conscrypt.provider.enable": "false",
      
      "spark:spark.sql.adaptive.enabled": "true",
      "spark:spark.sql.adaptive.coalescePartitions.enabled": "true",
      "yarn:yarn.app.mapreduce.am.resource.mb": "6000",
      "spark:spark.driver.stopOnSuccess.enabled": "true",
      "spark:spark.submit.deployMode": "cluster"
    }
  },
  "endpoint_config": {
    "enable_http_port_access": true
  },
   "lifecycle_config": {
    "auto_delete_ttl" : "28800s"
  }
}